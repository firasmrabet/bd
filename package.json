{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "postinstall": "npm rebuild && npm run clean-modules || true",
    "clean-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "dev": "vite",
    "start": "NODE_ENV=production node server.js",
    "build": "tsc --skipLibCheck && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server.js",
    "test:verify-update": "node test/verify-update-product.mjs",
    "clean": "rimraf dist node_modules",
    "vercel-build": "tsc --skipLibCheck && vite build",
    "prebuild": "npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "chrome-aws-lambda": "^10.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "googleapis": "^129.0.0",
    "html-pdf": "^3.0.1",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.17.1",
    "puppeteer": "^21.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.0",
    "stream-buffers": "^3.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
